// custom css
// support stylus
// check
// node_modules/ringcentral-embeddable-extension-common/src/spa/style.styl
// for overides or as example

.rc-contact-frame
  width 100%
  height 500px
  background #fff

.call-with-ringccentral-btn
  padding 1px
  border-radius 8px
  height 32px

#HubSpot-rc
  overflow visible
.text-center .call-with-ringccentral-btn
  margin 5px 0
.wordbreak
  word-break break-all

.rc-call-dds
  width 160px
.flex
  display flex
  .rc-input
    width 100%
  .flex-item
    flex-grow 1
.flex-item + .flex-item
  margin-left 2%
.rc-meet-icon
  line-height 24px
  width 24px
  text-align center
  margin 0 1px
  border 1px solid #fff
  border-radius 2px
  cursor pointer
  &:hover
    border 1px solid #eee
  svg
    display inline-block
    vertical-align middle
.rc-meet-shade
  position absolute
  left 0
  top 0
  width 100%
  height 100%
  z-index 2221
  background rgba(0,0,0,.5)
.rc-has-dd
  width 136px
*[class*="ant-"].ant-notification
  z-index 3000

.rc-drag-wrap
  position fixed
  left 3px
  bottom 3px
  z-index 777